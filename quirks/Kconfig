config UNPACKDIR
	bool
	prompt "UNPACKDIR (kirkstone, scarthgap)"
	depends on BRANCH_KIRKSTONE || BRANCH_SCARTHGAP
	default y

config KAS_INCLUDE_UNPACKDIR
        string
        default "quirks/unpackdir.yaml" if UNPACKDIR
